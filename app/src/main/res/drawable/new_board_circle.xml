<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="55dp"
                android:height="55dp" />
            <gradient
                android:endColor="#3023ae"
                android:startColor="#c86dd7" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <size
                android:width="55dp"
                android:height="55dp" />
            <solid android:color="#fff" />
        </shape>
    </item>
</layer-list>